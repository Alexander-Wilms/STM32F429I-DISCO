#include "stm32f4xx.h"
#include "stm32f429xx.h"
#include "stm32f429i_discovery.h"
#include "stm32f4xx_hal_dac.h"
			

int main(void)
{
	//HAL_Init();
	DAC_HandleTypeDef init;
	init.Instance = CR;
	HAL_DAC_Init(&init);
	for(;;);
}
